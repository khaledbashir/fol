generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Project {
  id          String   @id @default(cuid())
  title       String
  client      String
  description String
  challenge   String
  solution    String
  results     String
  technologies String[]
  sector      String
  image       String?
  featured    Boolean  @default(false)
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
